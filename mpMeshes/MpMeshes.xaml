                                <TextBlock Name="TbThirdRound" VerticalAlignment="Center" Text="3" Margin="5,0,0,0"></TextBlock>
                            </StackPanel>
                        </StackPanel>
                    </Border>
                    <Button Grid.Row="2" Content="{Binding Source={StaticResource Lang}, XPath=insert}"
